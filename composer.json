{
    "name": "daaquan/gmo-coin-php-sdk",
    "description": "Laravel wrapper for GMO Coin cryptocurrency and FX APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GMO Coin SDK",
            "email": "daaquan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-curl": "*",
        "textalk/websocket": "^1.5"
    },
    "scripts": {
        "phpstan": "phpstan analyse"
    },
    "autoload": {
        "psr-4": {
            "GmoCoin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GmoCoin\\GmoCoinFxServiceProvider",
                "GmoCoin\\GmoCoinCryptoServiceProvider"
            ],
            "aliases": {
                "GmoCoinFx": "GmoCoin\\Facades\\GmoCoinFx",
                "GmoCoinCrypto": "GmoCoin\\Facades\\GmoCoinCrypto"
            }
        }
    },
    "require-dev": {
        "laravel/framework": "^12.19",
        "phpunit/phpunit": "^10.1.16",
        "phpstan/phpstan": "^2.1"
    }
}
